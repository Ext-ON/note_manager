username = input("Имя пользователя")
print("Имя пользователя: ", username)
title = input("Заголовок заметки")
print("Заголовок заметки: " , title)
content = input("Описание заметки")
print("Описание заметки: ", content)
status = input("Статус заметки")
print("Статус заметки: ", status)
created_date = input('Дата создания заметки в формате "01.01.2000"')
print('Дата создания заметки в формате "01.01.2000"', created_date)
issue_date = input('Дата истечения заметки(дедлайн) в формате "01.01.2000"')
print('Дата истечения заметки(дедлайн) в формате "01.01.2000"', issue_date)
